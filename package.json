  },
  "homepage": "https://github.com/telemark/micro-portalen-shortcuts",
  "now": {
    "alias": [
      "shortcuts.portalen.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "axios": "0.16.2",
    "coveralls": "2.13.1",
