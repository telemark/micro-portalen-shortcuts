{
  "name": "micro-portalen-shortcuts",
  "description": "Mapping of shortcuts for portalen",
  "version": "1.2.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.7.1"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "shortcuts",
    "portalen",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-portalen-shortcuts.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-portalen-shortcuts/issues"
  },
  "now": {
    "alias": [
      "shortcuts.portalen.t-fk.win",
      "shortcuts.portalen.micro.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "axios": "0.15.3",
    "coveralls": "2.11.16",
    "nsp": "2.6.2",
    "nyc": "10.1.2",
    "standard": "8.6.0",
    "test-listen": "1.0.1"
  },
  "dependencies": {
    "is-file": "1.0.0",
    "marked": "0.3.6",
    "micro": "7.3.0",
    "tfk-unique-array": "1.0.11"
  }
}